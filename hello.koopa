fun @main(): i32 {
%entry:
  @t0 = alloc i32
  %0 = eq 2, 0
  br %0, %then_0, %else_0

%then_0:
  @t1 = alloc i32
  %1 = ne 6, 0
  br %1, %then_1, %else_1

%then_1:
  %2 = ne 12, 0
  store %2, @t1
  jump %end_1

%else_1:
  store 0, @t1
  jump %end_1

%end_1:
  %3 = load @t1
  %4 = ne 1, 0
  store %4, @t0
  jump %end_0

%else_0:
  store 1, @t0
  jump %end_0

%end_0:
  %5 = load @t0
  ret 3
}
