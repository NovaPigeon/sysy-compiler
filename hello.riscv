  .text
  .globl main
main:

  # prologue
  addi sp, sp, -16

  # alloc

  # store
  li t0, 10
  sw t0, 0(sp)

  # load
  lw t0, 0(sp)
  sw t0, 4(sp)

  # binary
  lw t0, 4(sp)
  li t1, 1
  add t2, t0, t1
  sw t2, 8(sp)

  # store
  lw t0, 8(sp)
  sw t0, 0(sp)

  # load
  lw t0, 0(sp)
  sw t0, 12(sp)

  # ret
  lw t0, 12(sp)
  mv a0, t0

  # epilogue
  addi sp, sp, 16
  ret
